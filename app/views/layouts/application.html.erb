<!DOCTYPE html>
<html>
<head>


  <title><%= @title %> - .Course+</title>

  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag   :defaults,"bootstrap","bootstrap.min","bootstrap-dropdown", "rails.validations","bootstrap-tooltip","bootstrap-popover","jquery.timePicker"  %>

  <%= csrf_meta_tag %>

</head>

<body >

<%= render 'layouts/header' %>

<%= link_to "Feedback", "http://coursex.userecho.com/" ,{ :class => "feedback"}%>


<div  class="container-fluid">

  <%= render :partial => "shared/flash_messages", :locals => {:flash => flash}  %>

  <%= yield %>


</div>

<%= render 'layouts/footer' %>

<%= debug(params) if Rails.env.development? %>
</body>
</html>
