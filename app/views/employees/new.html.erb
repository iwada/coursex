
<div class="row-fluid" >
  <% if current_user.admin? %>
   <%=  render 'sidebar' %>
      <% else %>
      
      <%= render 'newemployeesidebar' %>
      
     <% end %>
  <div class="span9" >
    <div class="hero-unit">
      <%= form_for @employee   do |f|%>
          <%= render 'form' ,:f => f%>

          <div class="actions">
          <%= f.submit "I'm Done",:class=> "btn btn-primary" %>
          <%= link_to 'Cancel', root_path,:class => "btn" %>
          </div>

      <% end %>
    </div>
  </div>

</div>

