<div class="row-fluid" >

  <%= render "employeesidebar" %>


  <div class="span9">
    <%= form_for @employee, :url => {:action =>"register"} do |f|%>

    <% @temp.each do |temp|%>

    <div class="hero-unit">
      <div class="span5">
        <%= temp.name %>
      </div>

      <div class="span5">

        <%= f.text_field :temp, :value=>temp.description %>
        <%= f.text_field :temp, :value=>temp.description %>
        <%= f.text_field :temp, :value=>temp.description %> <br />

        <%= submit_tag phrase(temp.id),:class=> "btn btn-primary" ,:disabled => false %>
        <%= link_to 'Cancel', employees_dashboard_path,:class => "btn" %>
      </div>

    </div>
    <% end %>
    <% end %>


  </div>

</div>