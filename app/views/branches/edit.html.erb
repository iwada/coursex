<div class="row-fluid" >

  <%= render 'sidebar' %>

  <div class="span9">
    <div class="hero-unit">

      <%= form_for @branch do |f|%>

          <%= render 'form' ,:f => f%>

          <%= f.submit :class=> "btn btn-primary" %>
          <%= link_to "Delete Branch" , @branch, :method =>:delete,:confirm =>"You Sure You want to delete this branch?",
                      :title => "Delete #{@branch.name}",:class => "btn btn-danger" %>
          <%= link_to 'Cancel', branches_path,:class => "btn" %>

      <% end %>
    </div>
  </div>
</div>